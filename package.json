{
  "name": "AutoStegano",
  "version": "1.0.0",
  "description": "Automatically compress and hide image data",
  "main": "ts/app.tsx",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode=production",
    "build-all": "cd svd-image-compression-worker && wasm-pack build && npm run build",
    "lint:check": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx,json}\" \"*.{js,json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,json}\" \"*.{js,json}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anonymousaaaardvark/autostegano.git"
  },
  "author": "Andrew Yang",
  "license": "MIT",
  "homepage": "https://anonymousaaaardvark.github.io/autostegano",
  "dependencies": {
    "css-loader": "^6.7.3",
    "image-steganography-worker": "file:image-steganography-worker/pkg",
    "nouislider": "^15.7.0",
    "pica": "^9.0.1",
    "rc-tooltip": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-loading-icons": "^1.1.0",
    "style-loader": "^3.3.1",
    "svd-image-compression-worker": "file:svd-image-compression-worker/pkg",
    "typescript-plugin-css-modules": "^4.2.2"
  },
  "devDependencies": {
    "@types/nouislider": "^9.0.10",
    "@types/pica": "^9.0.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-slick": "^0.23.10",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^5.0.0",
    "prettier": "2.8.4",
    "rc-slider": "^10.1.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "worker-loader": "^3.0.8"
  }
}
